{
  "name": "chessx",
  "version": "0.0.0",
  "description": "A dependency-free, customizable, and accessible JS chessboard.",
  "author": "Milind Ganjoo",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest --watch",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "stylelint": "stylelint \"**/*.css\"",
    "stylelint:fix": "stylelint --fix \"**/*.css\"",
    "types:check": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-interactions": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/html": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/testing-library": "^0.0.7",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-jest-dom": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.5",
    "eslint-plugin-testing-library": "^5.0.1",
    "fast-check": "^2.20.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,json,yml,yaml}": "prettier --write",
    "*.{css}": "stylelint --fix"
  }
}
